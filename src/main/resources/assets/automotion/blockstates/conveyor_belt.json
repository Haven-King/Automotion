    {
      "when": {"angle": "flat", "kind": "left|single", "facing": "north", "bottom": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_left_rail"}
    },
    {
      "when": {"angle": "flat", "kind": "right|single", "facing": "north", "bottom": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_right_rail"}
    },

    {
      "when": {"angle": "flat", "kind": "left|single", "facing": "north", "bottom": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_left_rail", "y": 180, "x": 180}
    },
    {
      "when": {"angle": "flat", "kind": "right|single", "facing": "north", "bottom": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_right_rail", "y": 180, "x": 180}
    },

    {
