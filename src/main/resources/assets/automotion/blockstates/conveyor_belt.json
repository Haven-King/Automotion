      "when": {"angle": "flat", "kind": "right|single", "facing": "west", "bottom": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_right_rail", "y": 270}
    },

    {
      "when": {"angle": "flat", "kind": "left|single", "facing": "west", "bottom": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_left_rail", "y": 90, "x": 180}
    },
    {
      "when": {"angle": "flat", "kind": "right|single", "facing": "west", "bottom": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_right_rail", "y": 90, "x": 180}
    },

    {
      "when": {"angle": "flat", "facing": "north", "enabled": "true", "bottom": "true"},
