    },
    {
      "when": {"facing": "north", "angle": "up", "enabled": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_belt_disabled"}
    },
    {
      "when": {"facing": "north", "angle": "up", "kind": "left|single"},
