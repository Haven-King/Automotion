{
  "multipart": [
    {
      "when": {"angle": "flat", "kind": "left|single", "facing": "north"},
      "apply": {"model": "automotion:block/conveyor_belt_left_rail"}
    },
    {
      "when": {"angle": "flat", "kind": "right|single", "facing": "north"},
      "apply": {"model": "automotion:block/conveyor_belt_right_rail"}
    },

    {
      "when": {"angle": "flat", "kind": "left|single", "facing": "east"},
      "apply": {"model": "automotion:block/conveyor_belt_left_rail", "y": 90}
    },
    {
      "when": {"angle": "flat", "kind": "right|single", "facing": "east"},
      "apply": {"model": "automotion:block/conveyor_belt_right_rail", "y": 90}
    },

    {
      "when": {"angle": "flat", "kind": "left|single", "facing": "south"},
      "apply": {"model": "automotion:block/conveyor_belt_left_rail", "y": 180}
    },
    {
      "when": {"angle": "flat", "kind": "right|single", "facing": "south"},
      "apply": {"model": "automotion:block/conveyor_belt_right_rail", "y": 180}
    },

    {
      "when": {"angle": "flat", "kind": "left|single", "facing": "west"},
      "apply": {"model": "automotion:block/conveyor_belt_left_rail", "y": 270}
    },
    {
      "when": {"angle": "flat", "kind": "right|single", "facing": "west"},
      "apply": {"model": "automotion:block/conveyor_belt_right_rail", "y": 270}
    },

    {
      "when": {"angle": "flat", "facing": "north", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_belt"}
    },
    {
      "when": {"angle": "flat", "facing": "east", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_belt", "y": 90}
    },
    {
      "when": {"angle": "flat", "facing": "south", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_belt", "y": 180}
    },
    {
      "when": {"angle": "flat", "facing": "west", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_belt", "y": 270}
    },

    {
      "when": {"angle": "flat", "facing": "north", "enabled": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_belt_disabled"}
    },
    {
      "when": {"angle": "flat", "facing": "east", "enabled": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_belt_disabled", "y": 90}
    },
    {
      "when": {"angle": "flat", "facing": "south", "enabled": "false"
      },
      "apply": {"model": "automotion:block/conveyor_belt_belt_disabled", "y": 180}
    },
    {
      "when": {"angle": "flat", "facing": "west", "enabled": "false"
      },
      "apply": {"model": "automotion:block/conveyor_belt_belt_disabled", "y": 270}
    },

    {
      "when": {"angle": "flat", "facing": "north"},
      "apply": {"model": "automotion:block/conveyor_belt_bottom"}
    },
    {
      "when": {"angle": "flat", "facing": "east"},
      "apply": {"model": "automotion:block/conveyor_belt_bottom", "y": 90}
    },
    {
      "when": {"angle": "flat", "facing": "south"
      },
      "apply": {"model": "automotion:block/conveyor_belt_bottom", "y": 180}
    },
    {
      "when": {"angle": "flat", "facing": "west"
      },
      "apply": {"model": "automotion:block/conveyor_belt_bottom", "y": 270}
    },

    {
      "when": {"facing": "north", "angle": "up", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test"}
    },
    {
      "when": {"facing": "north", "angle": "up", "enabled": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test_disabled"}
    },
    {
      "when": {"facing": "north", "angle": "up", "kind": "left|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_left_rail"}
    },
    {
      "when": {"facing": "north", "angle": "up", "kind": "right|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_right_rail"}
    },

    {
      "when": {"facing": "east", "angle": "up", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test", "y": 90}
    },
    {
      "when": {"facing": "east", "angle": "up", "enabled": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test_disabled", "y": 90}
    },
    {
      "when": {"facing": "east", "angle": "up", "kind": "left|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_left_rail", "y": 90}
    },
    {
      "when": {"facing": "east", "angle": "up", "kind": "right|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_right_rail", "y": 90}
    },

    {
      "when": {"facing": "south", "angle": "up", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test", "y": 180}
    },
    {
      "when": {"facing": "south", "angle": "up", "enabled": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test_disabled", "y": 180}
    },
    {
      "when": {"facing": "south", "angle": "up", "kind": "left|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_left_rail", "y": 180}
    },
    {
      "when": {"facing": "south", "angle": "up", "kind": "right|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_right_rail", "y": 180}
    },

    {
      "when": {"facing": "west", "angle": "up", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test", "y": 270}
    },
    {
      "when": {"facing": "west", "angle": "up", "enabled": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test_disabled", "y": 270}
    },
    {
      "when": {"facing": "west", "angle": "up", "kind": "left|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_left_rail", "y": 270}
    },
    {
      "when": {"facing": "west", "angle": "up", "kind": "right|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_right_rail", "y": 270}
    },

    {
      "when": {"facing": "north", "angle": "down", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test_reversed", "y": 180}
    },
    {
      "when": {"facing": "north", "angle": "down", "enabled": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test_disabled", "y": 180}
    },
    {
      "when": {"facing": "north", "angle": "down", "kind": "right|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_left_rail", "y": 180}
    },
    {
      "when": {"facing": "north", "angle": "down", "kind": "left|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_right_rail", "y": 180}
    },

    {
      "when": {"facing": "east", "angle": "down", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test_reversed", "y": 270}
    },
    {
      "when": {"facing": "east", "angle": "down", "enabled": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test_disabled", "y": 270}
    },
    {
      "when": {"facing": "east", "angle": "down", "kind": "right|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_left_rail", "y": 270}
    },
    {
      "when": {"facing": "east", "angle": "down", "kind": "left|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_right_rail", "y": 270}
    },

    {
      "when": {"facing": "south", "angle": "down", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test_reversed", "y": 0}
    },
    {
      "when": {"facing": "south", "angle": "down", "enabled": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test_disabled", "y": 0}
    },
    {
      "when": {"facing": "south", "angle": "down", "kind": "right|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_left_rail", "y": 0}
    },
    {
      "when": {"facing": "south", "angle": "down", "kind": "left|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_right_rail", "y": 0}
    },

    {
      "when": {"facing": "west", "angle": "down", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test_reversed", "y": 90}
    },
    {
      "when": {"facing": "west", "angle": "down", "enabled": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_test_disabled", "y": 90}
    },
    {
      "when": {"facing": "west", "angle": "down", "kind": "right|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_left_rail", "y": 90}
    },
    {
      "when": {"facing": "west", "angle": "down", "kind": "left|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_right_rail", "y": 90}
    }
  ]
}