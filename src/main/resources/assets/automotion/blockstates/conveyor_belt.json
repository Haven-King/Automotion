      "apply": {"model": "automotion:block/conveyor_belt_left_rail", "y": 90}
    },
    {
      "when": {"angle": "flat", "kind": "right|single", "facing": "east", "bottom": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_right_rail", "y": 90}
    },
    {
      "when": {"angle": "flat", "kind": "left|single", "facing": "east", "bottom": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_left_rail", "y": 270, "x": 180}
    },
    {
      "when": {"angle": "flat", "kind": "right|single", "facing": "east", "bottom": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_right_rail", "y": 270, "x": 180}
    },

    {
      "when": {"angle": "flat", "kind": "left|single", "facing": "south", "bottom": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_left_rail", "y": 180}
    },
    {
      "when": {"angle": "flat", "kind": "right|single", "facing": "south", "bottom": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_right_rail", "y": 180}
