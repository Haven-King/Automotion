    {
      "when": {"facing": "south", "angle": "down", "kind": "right|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_left_rail", "y": 0}
    },
    {
      "when": {"facing": "south", "angle": "down", "kind": "left|single"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_right_rail", "y": 0}
    },

    {
      "when": {"facing": "west", "angle": "down", "enabled": "true"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_belt_reversed", "y": 90}
    },
    {
      "when": {"facing": "west", "angle": "down", "enabled": "false"},
      "apply": {"model": "automotion:block/conveyor_belt_angled_belt_disabled", "y": 90}
