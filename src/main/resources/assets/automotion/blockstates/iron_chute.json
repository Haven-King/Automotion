{
  "multipart": [
    {
      "apply": {"model": "automotion:block/chute_top_and_bottom", "y": 0}
    },

    {
      "when": {"north": "false"},
      "apply": {"model": "automotion:block/chute_wall", "y": 0}
    },
    {
      "when": {"east": "false"},
      "apply": {"model": "automotion:block/chute_wall", "y": 90}
    },
    {
      "when": {"south": "false"},
      "apply": {"model": "automotion:block/chute_wall", "y": 180}
    },
    {
      "when": {"west": "false"},
      "apply": {"model": "automotion:block/chute_wall", "y": 270}
    },

    {
      "when": {"north": "true"},
      "apply": {"model": "automotion:block/chute_wall_connected", "y": 0}
    },
    {
      "when": {"east": "true"},
      "apply": {"model": "automotion:block/chute_wall_connected", "y": 90}
    },
    {
      "when": {"south": "true"},
      "apply": {"model": "automotion:block/chute_wall_connected", "y": 180}
    },
    {
      "when": {"west": "true"},
      "apply": {"model": "automotion:block/chute_wall_connected", "y": 270}
    }
  ]
}